<% @photo_categories.each do |cat| %>
		<h3><%= cat.name %></h3>
		<ul class="gallery_basic" id="basicGallery">
			<% cat.photos.each do |photo| %>
				<% if photo.audit.store_id == @store.id %>
					<li><%= link_to image_tag(photo.photo.url(:medium)), photo, remote: true %></li>
				<% end %>
			<% end %>
		</ul>
<% end %> 