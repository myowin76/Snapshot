// alert("test");

text = $('.no-results').children('span').text();
owner_id = $('.no-results').closest('.brands-actions').prev().children('select').val();

$('.site-content').hide();
$('.overlay').html('<%= j render("form_ajax") %>');
$('#brand_name').val(text);
$('#brand_brand_owner_id').val(owner_id);


$('.page-header .close, #new_brand btn.cancel').click(function () {
	$('.overlay').empty();
	$('.site-content').show();
 });