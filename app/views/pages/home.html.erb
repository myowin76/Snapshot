<strong>Photos Search</strong>
<div style="background-color: #f1f1f1;">
<%= form_tag photos_path, :method => 'get' do %>
  <p>
  	<%= label_tag "Postcode" %>
    <%= text_field_tag :search, params[:postcode] %>
  </p>
  <p>  
  	<h3>Channels</h3>
    <% for channel in @channels %>
    	<%= check_box_tag "channel[]", channel.id %>
    	<%= channel.name %>
    <% end %>
  </p>  
  <p>
  	<h3>Categories</h3>  
    <% for category in @categories %>
    	<%= check_box_tag "category[]", category.id %>
    	<%= category.name %><br />
    <% end %>
  </p>
  <p>  
    <%= submit_tag "Search", :name => nil %>
  </p>
<% end %>
</div>




