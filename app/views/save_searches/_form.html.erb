<%= simple_form_for @save_search do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%#= f.input :params %>
    <%= f.hidden_field :user_id, :value=> current_user.id %>
    <%= f.hidden_field :params %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Save", :id => 'new_save_search', :class => 'small' %>
    <%= link_to "Cancel", root_path, :class=>"cancel btn small" %>
  </div>
<% end %>