<fieldset>
  <button type="submit" class="btn btn-danger clear-form">
    <i class="icon-refresh icon-white"></i> Clear
  </button>
  <button type="submit" class="btn btn-primary search">
    <i class="icon-search icon-white"></i> Search
  </button>
</fieldset>  

<div class="saved-search">
	<ul></ul>
  <%#= render @saved_searches unless current_user.save_searches.blank? %>
	<%= link_to "Save", new_save_search_path(:params => params[:search]), id: "new_link", :class => "btn btn-primary small", remote: true %>
	<%= link_to "Open Filters", save_searches_path, id: "open_link", :class => "btn btn-success small", remote: true %>
</div> 

