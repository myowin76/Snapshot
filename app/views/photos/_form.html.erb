<%= simple_form_for(@photo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :brand %>
    <%= f.association :audit %>
    <%= f.association :category %>
    <%= f.association :media_location %>
    <%= f.association :media_vehicle %>
    <%= f.association :media_type %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
