
text = $('.no-results').children('span').text();
$('.site-content').hide();
$('.overlay').html('<%= j render("form_ajax") %>');
$('#retailer_name').val(text);

$('.page-header .close, btn.cancel').click(function () {
	$('.overlay').empty();
	$('.site-content').show();
 }); 

$('.new_retailer').validate({
  rules: {
    retailer_name: {
      required: true
    },
    retailer_sector_id: {
      required: true
    }
  }
});