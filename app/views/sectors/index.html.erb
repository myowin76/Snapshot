
<div class="page-header">
  <h1>Sector List</h1>
</div>   

<table id="tbl-sectors" class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Desc</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% for sector in @sectors %>
      <tr>
        <td><%= sector.name %></td>
        <td><%= sector.desc %></td>
        <td><%= link_to "Show", sector %></td>
        <td><%= link_to "Edit", edit_sector_path(sector) %></td>
        <td><%= link_to "Destroy", sector, :confirm => 'Are you sure?', :method => :delete %></td>
      </tr>
    <% end %>
  </tbody>  
</table>

<p><%= link_to "New Sector", new_sector_path %></p>
