
<%= simple_form_for(@sector) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :desc %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-primary' %>
    <%= link_to "Back", sectors_path, :class => 'btn' %>
  </div>
<% end %>
