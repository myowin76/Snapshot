<h1>Listing media_locations</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @media_locations.each do |media_location| %>
  <tr>
    <td><%= media_location.name %></td>
    <td><%= media_location.description %></td>
    <td><%= link_to 'Show', media_location %></td>
    <td><%= link_to 'Edit', edit_media_location_path(media_location) %></td>
    <td><%= link_to 'Destroy', media_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Media location', new_media_location_path %>
